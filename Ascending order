
#include <stdio.h>
#include <stdlib.h>

int main (){

int i,j;
int array[] = {2,4,3,5};
int temp = 0;

for(i = 0 ; i <= 3 ; i++){
  for(j = i + 1 ; j <= 3 ; j++){
   if(array[i] > array[j] ){
      temp = array[i];
      array[i] = array[j];
      array[j] = temp;
   }
  }
}

for(i = 0 ; i <= 3 ; i++){
   printf(" %d",array[i]);
}


return 0;
}
