                        }

                    }

                    for(i = 0 ;i < size; i++){
                        for(j = 0 ; j < size ; j++){

                            r_matrix[i][j] = 0 ; 

                            for(z = 0 ; z < size ; z++){

                                r_matrix[i][j] += matrix1[i][z] * matrix2[j][z] ;
                            }
                        
