#include <stdio.h>
#include <stdlib.h>

int main(){
int i,j;
int array[] ={2,3,4,5};
int largest = array[0];
int second_largest = array[0];
for(i = 0 ; i <= 3 ; i++){
    if(largest < array[i]){
      largest = array[i];
    }
}
printf("largest value is %d\n",largest);

for(i = 0 ; i <= 3 ; i++){
    if(second_largest < array[i] && array[i] < largest){
      second_largest = array[i];
    }
}
printf("second largest value is %d",second_largest);
return 0;
}
